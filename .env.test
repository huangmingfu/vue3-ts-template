# 定义专门用于测试环境的环境变量：

# 环境
VITE_NODE_ENV=test

#端口号
VITE_APP_PORT = 5173

# 接口前缀
VITE_APP_BASE_API = '/test-api'

# 后端服务地址
VITE_APP_SERVICE_API = 'http://localhost:888'

# 是否删除debugger
VITE_DROP_DEBUGGER = false

# 是否删除console.log
VITE_DROP_CONSOLE = false

# 是否sourcemap
VITE_SOURCEMAP = true

# 打包路径
VITE_BASE_PATH = ./

# 输出路径
VITE_OUT_DIR = dist-test
